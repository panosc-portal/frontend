{
  "name": "panosc-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion-icons/remix-line": "^2.10.0",
    "@react-hook/window-size": "^3.0.7",
    "@react-keycloak/web": "^2.1.4",
    "@rebass/forms": "^4.0.6",
    "@rebass/preset": "^4.0.5",
    "@rebass/preset-material": "^4.0.5",
    "@styled-system/css": "^5.1.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "immer": "^8.0.1",
    "keycloak-js": "11.0.2",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.27.0",
    "normalize.css": "^8.0.1",
    "ramda": "^0.27.1",
    "rc-slider": "^9.7.2",
    "react": "^17.0.2",
    "react-cool-inview": "^1.1.9",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^2.3.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.15.5",
    "react-rangeslider": "^2.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-spinners": "^0.9.0",
    "react-swipeable": "^6.0.1",
    "rebass": "^4.0.7",
    "search-query-generator": "https://github.com/noobadmin/search-query-generator.git",
    "styled-components": "^5.2.1",
    "swr": "^0.3.0",
    "use-immer": "^0.4.2",
    "zustand": "^3.0.3"
  },
  "scripts": {
    "start": "BROWSER=none HOST=0.0.0.0 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "prettier",
      "better-styled-components",
      "import"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "better-styled-components/sort-declarations-alphabetically": 2,
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "internal"
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "external",
              "position": "before"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "react"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/helper-define-map": "^7.13.12",
    "@babel/preset-flow": "^7.12.1",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-essentials": "^6.0.22",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/node-logger": "^6.0.22",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.22",
    "babel-plugin-emotion": "^10.0.33",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "flow-bin": "^0.142.0",
    "prettier": "^2.0.5",
    "react-is": "^16.13.1",
    "typescript": "^3.9.6"
  }
}
